		while (!stop) {
			Scanner sc = new Scanner(System.in);
			System.out.println("연산자 입력(+,-)");
			String a = sc.next();
			
			if (a.equals("+")) {
				result++;				
				System.out.println("volume은:" +result);
			}
				
			if(a.equals("-")){
				result--;				
					System.out.println("volume은:"+result);
					continue ;
			}
		}
		Volume vl = new Volume();
		String b = vl.updown();
		

	}

}

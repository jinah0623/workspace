public class Volume{
	public String updown(int a) {

		
		if(a.equals("+")) {
			String vl = "볼륨이 올라갔습니다";
		}

		if(a.equals("-")){				
			String vl = "볼륨이 내려갔습니다.";
			//System.out.println("현재 볼륨은"+result+"입니다");
			continue ;
		}
	}
}

